#include<communicate.h>

char FeatureCode[] = "XD1010001";

void Send_FrameHead(void){
    putchar(0xE5);
}
void Send_FrameEnd(void){
    putchar(0xE6);
}


// 开始命令 0x05
void Send_Start(){
    Send_FrameHead();// 帧头

    putchar(0x05);

    putstr(FeatureCode);

    Send_FrameEnd();
}

